# include <cstdlib>
# include <ctime>
# include <iomanip>
# include <iostream>
# include <mpi.h>

int main(int argc, char *argv[]);

int main(int argc, char *argv[]){

int num_proc;
int myrank;

  MPI_Init(&argc, &argv);

  
  
  MPI_Comm_size(MPI_COMM_WORLD, &num_proc);
  MPI_Comm_rank(MPI_COMM_WORLD, &myrank);
  
  std::cout<<"From process" << myrank <<" out of" << num_proc << "Hello World!\n";
  
  MPI_Finalize();



return 0;
}
